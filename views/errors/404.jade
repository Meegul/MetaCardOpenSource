extends ../layout

block content
    .container
        if messages.success
            .alert.alert-success(role='alert')
                for success in messages.success
                    div= success.msg
        if messages.error
            .alert.alert-danger(role='alert')
                for error in messages.error
                    div= error.msg
        .panel
            .panel-body
                h1 404 error - #{title}